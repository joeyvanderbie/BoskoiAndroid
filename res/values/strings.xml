	<!-- Eula -->
	<string name="hello_eula">Boskoi is a guide to edible urban wild-food sources.
Before starting please read the following information.</string>
	<string name="eula_1">1. Be friendly</string>
	<string name="eula_1_sub">Ask permission when ownership is unclear</string>
	<string name="eula_2">2. Be generous</string>
	<string name="eula_2_sub">Only pick what you will eat. Leave most for others (or wildlife)</string>
	<string name="eula_3">3. Be alert</string>
	<string name="eula_3_sub">Be ware of toxicity and don\'t tread on other plants when picking</string>
	<string name="eula_4">4. Be careful</string>
	<string name="eula_4_sub">Don\'t put any rare or protected species on the map</string>
	<string name="license_title">Boskoi is Open Source</string>
